@model TalentoIT.Entities.proposta_skill

@{
    ViewData["Title"] = "Edit";
}

<h1>Editar</h1>

<h4>Proposta Skill</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="id_proposta_skill" />
            <div class="form-group">
                <label asp-for="id_skill" class="control-label">Skill:</label>
                <select asp-for="id_skill" class="form-control" asp-items="ViewBag.id_skill"></select>
                <span asp-validation-for="id_skill" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="id_proposta" class="control-label">Proposta:</label>
                <select asp-for="id_proposta" class="form-control" asp-items="ViewBag.id_proposta"></select>
                <span asp-validation-for="id_proposta" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Editar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

